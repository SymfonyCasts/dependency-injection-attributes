{% extends 'base.html.twig' %}

{% block title %}SmartTV Remote{% endblock %}

{% block body %}
    <div class="mx-auto max-w-5xl">
        <div class="bg-[#1B1B1D] w-[477px] mx-auto rounded-xl p-6">

            <div class="rounded-xl bg-[#0E0E0E] px-[80px] py-6 mb-12 text-center text-xl uppercase">
                {% for label, messages in app.flashes %}
                    {% for message in messages %}
                        {{ message }}
                    {% endfor %}
                {% else %}
                    Press a Button
                {% endfor %}
            </div>

            <form method="post">
                <div class="flex justify-center mb-5">
                    <button name="button" value="power" class="flex rounded-full border border-[#3F4241] hover:border-[#C33E21] w-[100px] h-[100px] justify-center items-center focus:bg-[#C33E21] group">
